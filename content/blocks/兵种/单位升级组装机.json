{
  // 模块类型：定义该物品归属为“单位组装机模块”，用于系统识别其功能类别
  "type": "UnitAssemblerModule",
  // 模块名称：标识该模块为“基础组装机组件”
  "name": "单位升级组装机",
  // 功能描述：核心作用有两个——1. 放置在组装机边界框旁时，将组装机等级提升1级；2. 可作为额外有效载荷输入接口
  "description": "制造相应零件，用于组装急射",
  // 尺寸参数：代表该模块占据的空间大小为3（具体单位需结合对应系统设定，如格子数、体积单位等）
  "size": 3,
  // 分类归属：将模块归类到“units（单位）”类别，用于物品管理和筛选
  "category": "units",
  // 解锁前置：需先完成“10j-00-地面安装舱”研究，才能解锁该模块的获取或使用权限
  "research": "小型实验基地",
  // 制作需求：制造该模块需消耗的材料及对应数量，共3种
  "requirements": [
    "劣质硅/80",  // 铁素体，220个
    "精致石墨/60",  // 辉石，120个
    "初级芯片/40"     // 锂，80个
  ],
  // 运行消耗：模块工作时，每单位时间消耗1点能量（能量单位需结合对应系统设定）
  "consumes": {
    "power": 1
  },
  // 研究成本乘数：后续与该模块相关的衍生研究，其成本会按0.3倍计算（即降低70%研究成本）
  "researchCostMultiplier": 0.5
}
