      "radius": 11, //半径
      "phase": 90,
      "stroke": 2,
      "layerOffset": -1,
      "color": "66FFE3C4"
    },
    {
      "type": "HoverPart",
      "mirror": true,
      //是否镜像
      "x": 6,
      "y": -4,  //坐标位置1:4
      "radius": 13, //半径
      "phase": 90,
      "stroke": 2,
      "layerOffset": -1,
      "color": "66FFE3C4"
    },
  ],
  "weapons": [ // 武器列表（定义单位搭载的武器系统）
      {
        "name": "none", // 武器挂载点名称，专属标识为"sei发射器"（推测为导弹发射器）
          "x": 0.0, // 武器在挂载点X轴的偏移量，0表示水平方向与挂载点完全对齐
          "y": 0.0, // 武器在挂载点Y轴的偏移量，0表示垂直方向与挂载点完全对齐
