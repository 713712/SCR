            "speed": 4, // 导弹基础飞行速度，4.2表示飞行速度中等
            "damage": 25, // 导弹基础伤害值，42表示单枚导弹伤害中等
            "properties": {
              "homingPower": 0.12, // 导弹追踪目标的能力，0.12表示弱追踪，仅能轻微调整方向
              "width": 8.0, // 导弹碰撞体宽度（像素），8表示导弹模型较细
              "height": 8.0, // 导弹碰撞体高度（像素），8表示导弹模型为方形
              "shrinkX": 0.0, // 导弹飞行过程中X轴方向的收缩系数，0表示全程不收缩
              "shrinkY": 0.0, // 导弹飞行过程中Y轴方向的收缩系数，0表示全程不收缩
              "drag": -0.003, // 导弹飞行阻力系数，负值表示导弹会轻微加速（模拟推进器效果）
              "homingRange": 120, // 导弹追踪目标的有效范围，80表示仅在80单位距离内才会追踪
              "keepVelocity": false, // 导弹是否继承发射单位的速度，false表示导弹以自身速度飞行
              "splashDamageRadius": 35.0, // 导弹爆炸的范围伤害半径，35表示爆炸覆盖范围较大
              "splashDamage": 10.0, // 导弹爆炸的范围伤害值，45表示范围伤害略高于单体伤害
              "lifetime": 62.0, // 导弹的最大飞行时间，62表示超过时间后导弹会自行爆炸/消失
              "trailColor": "F2E878CF", // 导弹飞行轨迹的颜色，引用颜色库中的"子弹黄色背景色"
              "backColor": "F2E878CF", // 导弹模型背面颜色，与轨迹色一致，视觉统一
              "frontColor": "F2E878CF", // 导弹模型正面颜色，引用颜色库中的"子弹黄色"，增强立体感
              "hitEffect": "shieldBreak", // 导弹命中目标时的爆炸特效，引用特效库中的"冲击波爆炸"
              "despawnEffect": "shieldBreak", // 导弹超时/失效时的爆炸特效，与命中特效一致
              "weaveScale": 8.0, // 导弹飞行轨迹的摆动幅度系数，8表示飞行路径有明显的左右摆动
              "weaveMag": 2.0 // 导弹轨迹摆动的频率系数，2表示摆动频率中等，路径呈平缓波浪形
            }
          }
        } 
  ],
