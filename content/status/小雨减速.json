{
  "type": "StatusEffect", // 状态效果类型，用于定义角色/单位获得的临时状态
  "name": "小雨减速", // 状态效果名称，此处译为“精力透支”或“虚弱”
  "properties": {
    "color": "Pal.sap", // 状态效果关联颜色，引用全局颜色库（Pal）中的“sap”色（通常为暗淡的绿色/灰色，体现虚弱感）
    "speedMultiplier": 0.9, // 移动速度倍率，0.7表示受此状态影响时速度降低30%
    "healthMultiplier": 1, // 生命值倍率，0.8表示受此状态影响时最大生命值/当前生命值降低20%
    "effect": "Fx.sapped", // 状态附加的视觉特效，引用特效库（Fx）中的“sapped”效果（如粒子环绕、颜色变暗等）
    "effectChance": 0.1 // 视觉特效触发概率，0.1表示每帧/每次判定有10%概率播放特效，避免特效过于密集
  }
}
